{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
        "url": "https://api.economicsobservatory.com/phl/grow?vega"
    },
    "title": {
        "text": "Philippines",
        "subtitle": "Growth, % | Source: PSA through ECO-API"
    },
    "width": 300,
    "height": 300,
    "mark": {
        "type": "line",
        "color": "navy"
    },
    "encoding": {
        "x": {
            "field": "date",
            "type": "temporal"
        },
        "y": {
            "field": "value",
            "type": "quantitative"
        }
    }
}
