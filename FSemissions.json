{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
 
  "title": {"text": "Change in UK emissions for key sectors (2021 to 2022"},
 
  "width": 500,
  "height": 350,
   
  "data": {
    "url": "https://raw.githubusercontent.com/clambo24/clambo24.github.io/main/FSemissions.csv"
  },
 
  "mark": {
    "type": "bar",
    "color": "#8C57CC"},
 
  "encoding": {
    "x": {"field": "Labels"},  
 
    "y": {
      "field": "2021-2022",
      "type": "quantitative"},
    
      "tooltip": [
        {"field": "% 2021-2022",
        "type": "nominal"}
      ]
    }
}
